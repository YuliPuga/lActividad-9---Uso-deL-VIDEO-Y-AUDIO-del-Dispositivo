<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.AUDIOcordova" version="1.5.0" xmlns="http://www.w3.org/ns/widgets" 
  xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <preference name="phonegap-version" value="cli-9.0.0" />
  <name>Uso del AUDIO Y VIDEO del Dispositivo 
</name>
  <description>
      Esta es una app de PhoneGap.
  </description>
  <author  href="http://cordova.io">
    
      Yulissa Elizabeth Rojas Puga
  </author>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1.3.4" />
  <plugin name="cordova-plugin-dialogs"/> 
  <plugin name="cordova-plugin-device"/>
  <plugin name="cordova-plugin-camera"/>
  <plugin name="cordova-plugin-media-capture" spec="~3.0.3" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
      <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
      <allow-intent href="itms:*" />
      <allow-intent href="itms-apps:*" />
  </platform>
</widget>